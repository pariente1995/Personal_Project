<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
	PUBLIC "-//mybatis.org/DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- 자바빈즈(DTO/VO) 등에만 적용됨. -->
		<!-- 컬렉션 속성(map)의 경우에는 CamelHashMap 클래스 사용 -->
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	
	<!-- alias 추가 -->
	<typeAliases>
		<typeAlias type="com.saeahga.community.common.CamelHashMap" alias="camelHashMap"></typeAlias>
	</typeAliases>
</configuration>